{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": ["error", "unix"],
    "no-shadow": "off",
    "implicit-arrow-linebreak": ["error", "beside"],
    "no-param-reassign": ["error", { "props": false }],
    "camelcase": "off",
    "no-underscore-dangle": "off",
    "no-new": "off"
  }
}
